#include<iostream>
using namespace std;
int main()
{
    int age, voting_age;
    cout<<"Enter Age of a user:";
    cin>>age;
    
    if(age>=18)
    {
        cout<<"\nYou're eligible for voting";
    }
    else
    cout<<"\nYou aren't eligible for voting.";
    
    voting_age = 18 - age;
   
    cout << "\nYears you still have to be eligible to vote = " << voting_age;
  
    
    return 0;
}
